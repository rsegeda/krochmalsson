{
  "name": "krochmalsson",
  "version": "1.2.3",
  "description": "Krochmalsson based on the Phaser-3 ts boilerplate",
  "keywords": [
    "Phaser",
    "Phaser 3",
    "Phaser3",
    "Html5-game",
    "Phaser-boilerplate",
    "TypeScript",
    "Webpack",
    "Phaser-tutorial",
    "Game Examples",
    "Phaser-game",
    "Phaser-framework",
    "Phaser3-isometric",
    "Phaser3-isometric-game",
    "Phaser3-isometric-example"
  ],
  "devDependencies": {
    "expose-loader": "0.7.5",
    "ts-loader": "5.3.3",
    "typescript": "3.2.4",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "phaser": "3.15.1",
    "phaser-isometric": "1.0.0"
  },
  "scripts": {
    "preinstall": "cd phaser-isometric && npm i && cd .. && npm link phaser-isometric/",
    "build": "npm --prefix phaser-isometric/ run jsdoc && cp phaser-isometric/out/phaser-isometric.d.ts lib/ && rm -rf phaser-isometric/out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rsegeda/krochmalsson.git"
  },
